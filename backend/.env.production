# 生产环境配置
NODE_ENV=production
PORT=3000

# 前端地址（生产环境下前后端合并）
FRONTEND_URL=http://localhost:3000

# JWT密钥（生产环境请修改为强密钥）
JWT_SECRET=your-super-secure-jwt-secret-for-production-please-change-this
JWT_EXPIRES_IN=7d

# 管理员账号（生产环境请修改）
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# 文件上传配置
MAX_FILE_SIZE=5242880
UPLOAD_PATH=uploads

# 数据存储路径
DATA_PATH=data